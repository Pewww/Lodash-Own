{
  "name": "lodash-own",
  "version": "0.2.31",
  "description": "Pewww's own Lodash library",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "test": "jest --config jestconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Pewww/Lodash-Own"
  },
  "keywords": [
    "lodash",
    "methods library",
    "underscore"
  ],
  "author": "Pewww <jung22337@naver.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Pewww/Lodash-Own"
  },
  "devDependencies": {
    "@types/jest": "^24.0.9",
    "jest": "^24.1.0",
    "prettier": "^1.16.1",
    "ts-jest": "^24.0.0",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.2.4"
  },
  "files": [
    "LICENSE",
    "package.json",
    "index.js",
    "dist"
  ],
  "dependencies": {
    "@types/node": "^11.13.8",
    "handlebars": "^4.1.2",
    "http-proxy-agent": "^2.1.0",
    "https-proxy-agent": "^2.2.1",
    "js-yaml": "^3.13.1",
    "typings": "^2.1.1"
  }
}
